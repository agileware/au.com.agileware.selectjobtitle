<?xml version="1.0"?>
<extension key="au.com.agileware.selectjobtitle" type="module">
  <file>selectjobtitle</file>
  <name>Change Job Title to Select List</name>
  <description>Changes the Contact, Job Title field to a select list of options, removing the free-text field.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Agileware</author>
    <email>support@agileware.com.au</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/agileware/au.com.agileware.selectjobtitle</url>
    <url desc="Documentation">https://github.com/agileware/au.com.agileware.selectjobtitle</url>
    <url desc="Support">https://agileware.com.au/contact</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-02-15</releaseDate>
  <version>1.2</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Selectjobtitle</namespace>
    <format>23.02.1</format>
    <angularModule>crmSelectjobtitle</angularModule>
  </civix>
  <mixins>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
</extension>
